{
  "name": "deel-task",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "dependencies": {
    "body-parser": "1.20.1",
    "express": "4.18.2",
    "express-async-errors": "^3.1.1",
    "sequelize": "6.28.0",
    "sqlite3": "5.1.4"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc-watch --onSuccess \"node dist/server.js\"",
    "start:prod": "node dist/server.js",
    "seed": "npm run build && node dist/scripts/seedDb.js",
    "lint": "eslint --ext .js,ts,jsx,tsx,cjs .",
    "format": "prettier --loglevel warn --write '*.{js,ts,jsx,tsx,json,md,yaml,yml,cjs}'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "concurrently": "7.6.0",
    "eslint": "8.33.0",
    "eslint-config-antomic": "2.1.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "tsc-watch": "^6.0.0",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "engineStrict": true,
  "lint-staged": {
    "*.{js,ts,jsx,tsx,cjs}": "eslint --cache --fix",
    "*.{js,ts,jsx,tsx,json,md,yaml,yml,cjs}": "prettier --write"
  }
}
